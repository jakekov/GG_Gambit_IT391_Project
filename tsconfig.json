{

  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ES2020",
    "outDir": "dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules, dist"]
}
